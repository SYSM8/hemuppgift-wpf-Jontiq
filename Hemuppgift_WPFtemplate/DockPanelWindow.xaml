<Window x:Class="Hemuppgift_WPFtemplate.DockPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockPanelWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Top" x:Name="TopButton" Content="Top" Height="30" Width="80" Margin="5"/>
            <Button DockPanel.Dock="Bottom" x:Name="BottomButton" Content="Bottom" Height="30" Width="80" Margin="5"/>
            <Button DockPanel.Dock="Left" x:Name="LeftButton" Content="Left" Height="30" Width="80" Margin="5"/>
            <Button DockPanel.Dock="Right" x:Name="RightButton" Content="Right" Height="30" Width="80" Margin="5"/>
            <Button x:Name="CenterButton" Content="Center" Height="30" Width="80" Margin="5" Click="CenterButton_Click"/>

        </DockPanel>
        <Image x:Name="Trophy" Source="/TrophyImage.jpg" Height="200" Width="200" Visibility="Hidden"/>

    </Grid>
</Window>
